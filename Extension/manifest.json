{
  "name": "Job Posting Scraper",
  "description": "Scrapes job postings for filtering based on job specific filters",
  "version": "1.0",
  "manifest_version": 3,

  "background": {
    "scraper": "scraper.js"
  },

  "permissions": ["storage"],

  "action": {
    "default_popup": "interface.html",

    "default_icon": {
      "16": "/assets/get_started16.png",
      "32": "/assets/get_started32.png",
      "48": "/assets/get_started48.png",
      "128": "/assets/get_started128.png"
    }
  },

  "icons": {
    "16": "/assets/get_started16.png",
    "32": "/assets/get_started32.png",
    "48": "/assets/get_started48.png",
    "128": "/assets/get_started128.png"
  }

}